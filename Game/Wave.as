	break;									case ColorModes.Green:					mColor = 3;					break;			}		}				public static function generate(num:int):Vector.<Wave>		{			var waves:Vector.<Wave> = new Vector.<Wave>(num);			var w:Wave;			var i:int;						for(i=0; i<num; i++)			{				w = List[i];								waves[i] = new Wave(w.num, w.name, w.remaining, w.spawnInterval, w.type, w.color, w.tunnelX, w.tunnelY);			}			return waves;		}				private static const List:Vector.<Wave> = new Vector.<Wave>(100, true);			List[ 0] = new Wave(1, "normal",   20, 20,  1, ColorModes.Blue);		List[ 1] = new Wave(2, "normal",   20, 20,  2, ColorModes.Blue);		List[ 2] = new Wave(3, "normal",   20, 20,  3, ColorModes.Blue);		List[ 3] = new Wave(4, "normal",   20, 20,  4, ColorModes.Blue);		List[ 4] = new Wave(5, "flyers",   20, 20,  5, ColorModes.Blue);		List[ 5] = new Wave(6, "normal",   20, 20,  6, ColorModes.Red);		List[ 6] = new Wave(7, "spawners", 15, 20,  7, ColorModes.Red);				List[ 7] = new Wave(8, "normal",   20, 20,  8, ColorModes.Red);		List[ 8] = new Wave(9, "spawners", 20, 20,  9, ColorModes.Black);		List[ 9] = new Wave(10, "boss",     1, 20, 13, ColorModes.Black);		List[10] = new Wave(11, "spawners",20, 20,  7, ColorModes.Black);		List[11] = new Wave(12, "flyers",  20, 20,  5, ColorModes.Green);		List[12] = new Wave(13, "spawners",20, 20,  7, ColorModes.Green);		List[13] = new Wave(14, "normal",  20, 20,  8, ColorModes.Green);		List[14] = new Wave(15, "normal",  20, 20,  6, ColorModes.Red);		List[15] = new Wave(16, "normal",  20, 20,  3, ColorModes.Red);		List[16] = new Wave(17, "normal",  20, 20,  8, ColorModes.Black);		List[17] = new Wave(18, "fliers",  20, 20,  5, ColorModes.Green);		List[18] = new Wave(19, "spawners",20, 20,  7, ColorModes.Green);		List[19] = new Wave(20, "normal",  20, 20,  5, ColorModes.Green);		List[20] = new Wave(21, "spawners",20, 20,  7, ColorModes.Green);		List[21] = new Wave(22, "normal",  20, 20,  8, ColorModes.Red);		List[22] = new Wave(23, 
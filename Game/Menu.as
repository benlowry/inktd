package Game{	import flash.display.BitmapData;	import flash.events.MouseEvent;	import flash.net.URLRequest;	import flash.net.navigateToURL;	import Playtomic.Log;		public final class Menu extends BaseScreen	{				public function Menu(image:BitmapData)		{			super(image, initializeComplete);		}				private function initializeComplete():void		{			playButton.label.text = "PLAY";			playButton.addEventListener(MouseEvent.CLICK, play, false, 0, true);						playtomicButton.label.text = "PLAYTOMIC";			playtomicButton.addEventListener(MouseEvent.CLICK, playtomic, false, 0, true);		}							private function play(e:MouseEvent):void		{			// Here we log our play			Log.Play();						parent.addChild(new Mode(Screenshot.create()));			parent.removeChild(this);		}				private function playtomic(e:MouseEvent):void		{			navigateToURL(new URLRequest("http://playtomic.com/?inked"));		}	}}